# Test mangling the "self" variable
test("
@export
class Foo {
  var x = 100

  def foo fn() int {
    var y = dynamic.get()
    return => x + y
  }

  def bar fn() int {
    var y = dynamic.get()
    while true {
      return => x + y
    }
  }

  def baz fn() int {
    while true {
      return => x
    }
  }

  def test fn() int {
    return => x
  }
}
", "
(function() {
  Foo = function() {
    this.x = 100;
  };

  Foo.prototype.foo = function() {
    var b = this, a = get();
    return function() {
      return b.x + a;
    };
  };

  Foo.prototype.bar = function() {
    for (var b = this, a = get();;) {
      return function() {
        return b.x + a;
      };
    }
  };

  Foo.prototype.baz = function() {
    for (var a = this;;) {
      return function() {
        return a.x;
      };
    }
  };

  Foo.prototype.test = function() {
    var a = this;
    return function() {
      return a.x;
    };
  };
})();
").jsMangle

